{
    "name": "react-dsfr-tiptap-examples",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "predev": "react-dsfr update-icons",
        "dev": "vite",
        "prebuild": "react-dsfr update-icons",
        "build": "tsc -b && vite build",
